{
  "name": "hono-starter",
  "type": "module",
  "scripts": {
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "dev": "bun run --watch src/index.ts",
    "format": "biome format",
    "format:fix": "biome format --write",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "typecheck": "tsc",
    "dedupe": "rm -rf node_modules && rm bun.lock && bun install",
    "up": "bunx npm-check-updates -ui"
  },
  "dependencies": {
    "@hono/zod-openapi": "1.1.4",
    "@paralleldrive/cuid2": "3.1.0",
    "@scalar/hono-api-reference": "0.9.22",
    "@scalar/openapi-to-markdown": "0.2.45",
    "@t3-oss/env-core": "0.13.8",
    "better-auth": "1.3.28",
    "dotenv": "17.2.3",
    "drizzle-orm": "0.44.6",
    "drizzle-zod": "0.8.3",
    "hono": "4.10.1",
    "openapi-merge": "1.3.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@types/bun": "1.3.0",
    "better-sqlite3": "12.4.1",
    "drizzle-kit": "0.31.5",
    "knip": "5.66.0",
    "typescript": "5.9.3"
  },
  "private": true
}
